{
    "manifest_version": 2,
    "name": "BackToTop",
    "version": "1.0.0",
    "description": "Back to top for github.",
    "content_scripts": [
        {
            "matches": ["https://*.github.com/*"],
            "css": ["backtotop.css"],
            "js": ["jquery.js", "backtotop.js"],
            "run_at": "document_end"
        }
    ]
}